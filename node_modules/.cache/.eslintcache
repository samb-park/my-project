patch } from \"react-redux\";\nimport { display } from \"../../module/updateHeader\";\n\nimport parse from \"html-react-parser\";\nimport styles from \"./ProductDetail.module.css\";\nimport useForm from \"../../hook/useForm\";\nimport validate from \"../../validation/Validation\";\nimport Button from \"../UI/Button\";\nimport InputText from \"../UI/InputText\";\nimport RadioGroup from \"../UI/RadioGroup\";\n\nconst ProductDetail = ({ products }) => {\n    const { id } = useParams();\n    const dispatch = useDispatch();\n    const index = products.findIndex((x) => x.id === Number(id));\n    const product = products[index];\n    const { handleChange, handleSubmit, values, errors } = useForm(\n        validate,\n        product\n    );\n\n    useEffect(() => {\n        dispatch(\n            display({\n                menu: \"Shop\",\n                name: \"Vitamins & Supplements\",\n                title: product.name,\n                description: product.meta_description,\n            })\n        );\n    }, []);\n\n    return (\n        <section>\n            <div className={styles.productDetail}>\n                <div className=\"image pt-5\">\n                    <img\n                        src={product.primary_image.url_standard}\n                        alt={product.primary_image.url_standard}\n                    />\n                </div>\n                <div className=\"description pt-5\">\n                    {parse(product.description)}\n                    <p className=\"text-base lg:text-2xl text-right mb-3\">\n                        USD ${values.purchace}\n                    </p>\n                </div>\n                <div className=\"form pt-5 w-full col-span-2\">\n                    <form onSubmit={handleSubmit}>\n                        <RadioGroup\n                            values={values}\n                            price={product.price}\n                            handleChange={handleChange}\n                        />\n                        <div className=\"my-5\">\n                            <InputText\n                                text=\"Who it's for:\"\n                                name=\"customername\"\n                                value={values.customername}\n                                onChange={handleChange}\n                            />\n                            {errors && (\n                                <p className=\" text-red-600\">\n                                    {errors.customername}\n                                </p>\n                            )}\n                            <InputText\n                                text=\"List any allergies :\"\n                                name=\"allergies\"\n                                value={values.allergies}\n                                onChange={handleChange}\n                            />\n                        </div>\n                        <Button type=\"submit\" name=\"Add to Cart\" />\n                    </form>\n                </div>\n            </div>\n        </section>\n    );\n};\n\nexport default ProductDetail;\n","/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/hook/useForm.js",[],[],"/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/validation/Validation.js",[],[],"/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/module/index.js",[],[],"/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/module/addCart.js",[],[],"/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/module/updateHeader.js",[],[],"/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/components/UI/Button.js",[],[],"/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/components/UI/InputText.js",[],[],"/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/components/UI/RadioGroup.js",[],[],"/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/components/UI/Badge.js",[],[],"/Users/sangbongpark/Documents/GitHub/TakeHome/birdandbe/src/hook/useFetchData.js",["217"],[],"import React from \"react\";\r\nimport { useState, useEffect } from \"react\";\r\n\r\nconst useFetchData = (url) => {\r\n    const [data, setData] = useState([]);\r\n    const [error, setError] = useState(false);\r\n    const [isLoading, setIsLoading] = useState(false);\r\n\r\n    useEffect(() => {\r\n        const getData = async () => {\r\n            try {\r\n                setIsLoading(true);\r\n                const response = await fetch(url);\r\n                const responseData = await response.json();\r\n                setData(responseData.products);\r\n            } catch (error) {\r\n                setError(error);\r\n            } finally {\r\n                setIsLoading(false);\r\n            }\r\n        };\r\n\r\n        getData();\r\n    }, [url]);\r\n\r\n\r\n    return { data, error, isLoading };\r\n};\r\n\r\nexport default useFetchData;\r\n","/Users/sangbongpark/Documents/GitHub/my-project/src/index.js",[],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/App.js",["218","219","220"],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/module/index.js",[],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/hook/useFetchData.js",["221"],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/module/addCart.js",[],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/module/updateHeader.js",[],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/components/Shop/ProductDetail.js",["222"],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/components/Shop/Shop.js",[],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/components/Layout/Layout.js",[],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/components/Cart/Cart.js",["223"],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/components/Layout/Header.js",[],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/components/Shop/Product.js",["224"],[],"/Users/sangbongpark/Documents/GitHub/my-project/src/components/Layout/Navbar.js",